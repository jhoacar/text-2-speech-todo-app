{
    "root":true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
	    "no-console":"off", 
	    "react/react-in-jsx-scope":"off",
	    "import/prefer-default-export":"off"
    }
}
